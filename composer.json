{
    "name": "automattic/amp-wp",
    "description": "WordPress plugin for adding AMP support.",
    "homepage": "https://github.com/Automattic/amp-wp",
    "type": "wordpress-plugin",
    "license": "GPL-2.0",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/xwp/PHP-CSS-Parser"
        },
        {
            "type": "vcs",
            "url": "git@github.com:dennisinteractive/amp-library.git"
        }
    ],
    "require": {
        "php": "^5.3.6 || ^7.0",
        "sabberworm/php-css-parser": "dev-master as 8.0.0",
        "xwp/wp-dev-lib": "dev-master",
        "lullabot/amp": "dev-amphtml-generator",
        "symfony/console": "^3.4"
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "^0.14.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
        "wimg/php-compatibility": "^8.1"
    }
}
